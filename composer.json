{
    "name": "gintonicweb/users",
    "description": "Users plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "cakephp/cakephp": "~3.0",
        "admad/cakephp-jwt-auth": "1.0.*@dev",
        "friendsofcake/crud-users": "dev-master",
        "muffin/tokenize": "1.0.x-dev",
        "xety/cake3-cookieauth": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Users\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Users\\Test\\": "tests"
        }
    }
}
